   "source": [
    "# 3.矩阵和线性方程组\n",
    "import numpy as np\n",
    "\n",
    "A=np.array([[1,1,0],[-4,3,0],[1,0,2]])\n",
    "b=np.array([0,1,0])\n",
    "\n",
    "eigenvalues,eigenvectors=np.linalg.eig(A)\n",
    "print(\"特征值：\",eigenvalues)\n",
    "print(\"特征向量：\",eigenvectors)\n",
    "\n",
    "x=np.linalg.solve(A,b)\n",
    "print(\"线性方程组的解：\",x)"
   ]
  }
 ],
