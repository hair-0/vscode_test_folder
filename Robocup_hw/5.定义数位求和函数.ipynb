    "# 5.定义数位求和函数\n",
    "def THU(n):\n",
    "    while n>=10:\n",
    "        n=sum(int(digit)for digit in str(n))\n",
    "    return(n)\n",
    "\n",
    "print(\"输入29的结果：\",THU(29))"
   ]
